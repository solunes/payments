{
    "name": "solunes/payments",
    "description": "Paquete adicional de Pagos para Solunes Digital.",
    "keywords": ["laravel", "solunes", "payments","package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Solunes Digital",
            "email": "info@solunes.com"
        }
    ],
    "require": {
        "solunes/master": "*",
        "ignited/laravel-omnipay": "2.3",
        "omnipay/paypal": "*",
        "dercoder/omnipay-neteller": "*",
        "bileto/omnipay-payu": "^0.6.0",
        "cybersource/rest-client-php": "0.0.17",
        "omnipay/braintree": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "classmap": [
            "src/database/migrations",
            "src/app/Providers",
            "src/app/Controllers",
            "src/app/Console"
       ],
        "psr-4": {
            "Solunes\\Payments\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true
}
